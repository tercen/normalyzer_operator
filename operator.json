{
  "name": "Normalisation",
  "description": "Normalises data using various methods.",
  "tags": [
    "preprocessing",
    "transformation",
    "normalisation"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/normalyzer_operator"
  ],
  "container": "tercen/normalyzer_operator:0.1.1",
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "method",
      "defaultValue": "mean",
      "values": [
        "global intensity",
        "median",
        "mean",
        "vsn",
        "quantile",
        "smad",
        "cyclicLoess",
        "global RLR",
        "none"
      ],
      "description": "Normalisation method."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Variable",
            "type": "",
            "description": "Variable identifiers",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Observation",
            "type": "",
            "description": "Observation identifiers",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Measurement values for normalisation",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}